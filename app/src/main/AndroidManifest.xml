<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.clari.afinal">

    <uses-sdk
        android:maxSdkVersion="30"
        android:minSdkVersion="20"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Final">
        <activity android:name=".Multiplicar"></activity>
        <activity android:name=".Documentacion" />
        <activity android:name=".Raiz" />
        <activity android:name=".Potencia" />
        <activity android:name=".Division" />
        <activity android:name=".Resta" />
        <activity android:name=".Suma" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>